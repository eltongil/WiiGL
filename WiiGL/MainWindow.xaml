                        </Grid>
                    </TabItem>
                    <TabItem Header="Savitzky-Golay">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="29*"/>
                                <ColumnDefinition Width="55*"/>
                                <ColumnDefinition Width="36*"/>
                                <ColumnDefinition Width="55*"/>
                                <ColumnDefinition Width="38*"/>
                                <ColumnDefinition Width="55*"/>
                                <ColumnDefinition Width="36*"/>
                                <ColumnDefinition Width="55*"/>
                                <ColumnDefinition Width="93*"/>
