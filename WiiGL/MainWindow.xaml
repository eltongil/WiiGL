                            <Button x:Name="AplicarButter" Content="Aplicar" Click="AplicarButter_Click_1" Grid.Column="2"/>
                        </Grid>
                    </TabItem>
                </TabControl>
            </Grid>            
        </TabItem>
        <TabItem Header="Testes">
            <ScrollViewer HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
