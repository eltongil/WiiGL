                            <TextBox x:Name="SGK" PreviewTextInput="So_Numeros" Grid.Row="1" Grid.Column="5" />
                            <TextBox x:Name="SGI" PreviewTextInput="So_Numeros" Grid.Row="1" Grid.Column="7" />
