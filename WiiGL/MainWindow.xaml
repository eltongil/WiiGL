                    </TabItem>
                    <TabItem Header="Média bidirecional">
                        <Grid Margin="0,5,3,5" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Width="Auto" Height="Auto" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="74*"/>
                                <ColumnDefinition Width="433*"/>
                                <ColumnDefinition Width="60*"/>
                                <ColumnDefinition Width="455*"/>
